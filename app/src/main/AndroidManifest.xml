<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sushivalenciatfg">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.ComentariosActivity"
            android:exported="false" />
        <activity
            android:name=".activities.PerfilActivity"
            android:exported="false" />
        <activity
            android:name=".activities.NuevoRestauranteActivity"
            android:exported="false" />
        <activity
            android:name=".activities.InicioActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MasInfoActivity"
            android:exported="false" />
        <activity
            android:name=".activities.InfoRestauranteActivity"
            android:exported="false" />
        <activity
            android:name=".activities.RegistroActivity"
            android:exported="false" />
        <activity
            android:name=".activities.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".activities.MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>